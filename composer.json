{
  "name": "brightecapital/sns",
  "type": "library",
  "description": "Message Queue Amazon SNS Transport (fork and improve from enqueue/sns)",
  "keywords": [
    "messaging",
    "queue",
    "amazon",
    "aws",
    "sns"
  ],
  "homepage": "https://github.com/brightecapital/",
  "license": "MIT",
  "authors": [
    {
      "name": "Ken Ngo",
      "email": "ken.ngo@brighte.com.au"
    }
  ],
  "require": {
    "php": "^7.1.3",
    "queue-interop/queue-interop": "^0.8",
    "enqueue/dsn": "^0.10",
    "aws/aws-sdk-php": "~3.26"
  },
  "require-dev": {
    "phpunit/phpunit": "~7.5",
    "enqueue/test": "0.10.x-dev",
    "queue-interop/queue-spec": "^0.6"
  },
  "autoload": {
    "psr-4": {
      "Brighte\\Sns\\": "src/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "Brighte\\Sns\\Tests\\": "tests/"
    }
  },
  "minimum-stability": "dev"
}
